  "dialogue_turns": [
    {
      "turn_id": 1,
      "speaker": "Interviewer",
      "text": "I'd love to hear about your time touring in a van."
    },
    {
      "turn_id": 2,
      "speaker": "Subject",
      "sentences": [
        {
          "text": "Oh, those were the days!",
          "annotations": {
