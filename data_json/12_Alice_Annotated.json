  "dialogue_turns": [
    {
      "turn_id": 1,
      "speaker": "Interviewer",
      "text": "I'd love to hear about your early days as a copy girl in the newsroom."
    },
    {
      "turn_id": 2,
      "speaker": "Subject",
      "sentences": [
        {
          "text": "Oh, those were the days!",
          "annotations": {
            "life_stage": "Youth",
            "event": "Reflection",
            "emotions": ["Nostalgia/Gratitude"],
            "entities": []
          }
        },
        {
          "text": "I was just a young thing, fresh out of high school, eager to get my foot in the door.",
          "annotations": {
            "life_stage": "Youth",
            "event": "Career Start",
            "emotions": ["Joy/Pride", "Nostalgia/Gratitude"],
            "entities": [
              { "text": "high school", "type": "ORGANIZATION", "role": "education" }
            ]
          }
        },
        {
          "text": "The newsroom was a bustling place, always alive with the sound of typewriters clacking away.",
          "annotations": {
            "life_stage": "Youth",
            "event": "Atmosphere",
            "emotions": ["Excitement"],
