    {
      "turn_id": 4,
      "speaker": "Subject",
      "sentences": [
        {
          "text": "Oh dear, that was quite the ordeal.",
          "annotations": {
            "life_stage": "Youth",
            "event": "Reflection",
            "emotions": ["Fear/Anxiety"],
            "entities": []
          }
        },
        {
          "text": "I had been sent to deliver some documents to one of the top editors.",
